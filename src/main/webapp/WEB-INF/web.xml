        </init-param>
        <init-param>
            <param-name>password</param-name>
            <param-value></param-value>
        </init-param>
        <servlet-class>org.gyf.servlet.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
